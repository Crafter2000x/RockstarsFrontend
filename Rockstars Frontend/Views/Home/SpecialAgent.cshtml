@model FormulierEnAanvraagModel
<link rel="stylesheet" href="~/css/Specialagent.css" />
    <div class="container">
        <div class="row art" style="margin-left: -10%; margin-right: -10%;">

            @foreach (User user in Model.userlist)
            {
                string profilePicture = "https://localhost:6001" + user.Image.RelativeUrl;
                <div class="card user-card-full">
                    <div class="row m-l-0 m-r-0">
                        <div class="col-sm-4 bg-c-lite-green user-profile">
                            <div class="card-block text-center text-white">
                                <div class="m-b-25"> <img src="@profilePicture" class="img-radius card-img-top" alt="User-Profile-Image"> </div>
                                <h6 class="f-w-600">@user.Name</h6>
                            </div>
                        </div>
                        <div class="col-sm-8">
                            <div class="card-block">
                                <h6 class="m-b-20 p-b-5 b-b-default f-w-600">Biografie</h6>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <p class="m-b-10 f-w-600">@user.Biography</p>
                                    </div>
                                </div>
                                <h6 class="m-b-20 m-t-40 p-b-5 b-b-default f-w-600"></h6>
                                <div class="row">
                                    @if (user.LinkedIn != null)
                                    {
                                        <div class="col-sm-6">
                                                <p class="m-b-10 f-w-600">LinkedIn</p>
                                                <h6 class="text-muted f-w-400">
                                                    <a href="@user.LinkedIn">
                                                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" class="bi bi-linkedin" viewBox="0 0 16 16">
                                                            <path d="M0 1.146C0 .513.526 0 1.175 0h13.65C15.474 0 16 .513 16 1.146v13.708c0 .633-.526 1.146-1.175 1.146H1.175C.526 16 0 15.487 0 14.854V1.146zm4.943 12.248V6.169H2.542v7.225h2.401zm-1.2-8.212c.837 0 1.358-.554 1.358-1.248-.015-.709-.52-1.248-1.342-1.248-.822 0-1.359.54-1.359 1.248 0 .694.521 1.248 1.327 1.248h.016zm4.908 8.212V9.359c0-.216.016-.432.08-.586.173-.431.568-.878 1.232-.878.869 0 1.216.662 1.216 1.634v3.865h2.401V9.25c0-2.22-1.184-3.252-2.764-3.252-1.274 0-1.845.7-2.165 1.193v.025h-.016a5.54 5.54 0 0 1 .016-.025V6.169h-2.4c.03.678 0 7.225 0 7.225h2.4z" />
                                                        </svg>&nbsp; &nbsp;LinkedIn
                                                    </a>
                                                </h6>
                                        </div>
                                    }
                                            <div class="col-sm-6">
                                                <h1>
                                                    <button type="button" class="btn btn-primary moreButton" data-target="#PopupUpdatetabel1" data-toggle="modal">Contact</button>
                                                </h1>
                                                <div class="modal" id="PopupUpdatetabel1" role="dialog">
                                                    <div class="modal-dialog">
                                                        <div class="modal-content">
                                                            <div class="modal-header">
                                                                <h1 style="color:black;">Stel je vraag.</h1>
                                                                <button class="close" data-dismiss="modal">&times;</button>
                                                            </div>
                                                            @using (Html.BeginForm("HulpAanvragen", "Home", FormMethod.Post))
                                                            {
                                                                <div class="modal-body">
                                                                    <p>Stel hier je vraag aan de special agent</p>
                                                                    <p>@Html.TextBoxFor(m => Model.form.contactEmail, new { style = "width:100%", @autocomplete = "off", placeholder = "Voer het mailadres van de specialagent in" })</p>
                                                                    <p>@Html.TextBoxFor(m => Model.form.comment, new { style = "width:100%", @autocomplete = "off", placeholder = "Voer je vraag in" })</p>
                                                                </div>
                                                                <div class="modal-footer">
                                                                    <input type="submit" class="btn btn-primary" value="Stel vraag" />
                                                                </div>
                                                            }
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                            </div>
                        </div>
                    </div>
                </div>


